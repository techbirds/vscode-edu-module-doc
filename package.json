{
  "name": "vscode-edu-module-doc",
  "displayName": "vscode-edu-module-doc",
  "description": "Edu Module Doc",
  "version": "0.0.1",
  "publisher": "nedu",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/techbirds/vscode-edu-module-doc.git"
  },
  "activationEvents": [
    "*",
    "onCommand:extension.listProjectREADME",
    "onCommand:extension.listProjectCHANGELOG"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.listProjectREADME",
        "title": "Edu Module: List Project Open README.md"
      },
      {
        "command": "extension.listProjectCHANGELOG",
        "title": "Edu Module: List Project Open CHANGELOG.md"
      }
    ],
    "keybindings": [
      {
        "command": "extension.listProjectREADME",
        "key": "shift+alt+k",
        "mac": "cmd+alt+k"
      },
      {
        "command": "extension.listProjectCHANGELOG",
        "key": "ctrl+alt+k",
        "mac": "ctrl+cmd+k"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "jsonfile": "^4.0.0",
    "open": "0.0.5",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
